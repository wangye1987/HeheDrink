<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="150dp"
    android:alpha="0.92"
    android:background="@color/color_545151"
    android:id="@+id/call_root"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/linear_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/dialog"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/title_linear"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:background="@color/white"
            android:layout_marginTop="@dimen/margin_5"
            android:paddingLeft="@dimen/margin_10"
            android:paddingRight="@dimen/margin_10" >

            <TextView
                android:id="@+id/tv_order_resason"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_centerInParent="true"
                style="@style/gray_textstyle_14"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_order"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="请联系客服"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/text_size_14" />

            <LinearLayout
                android:id="@+id/linear_close"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                 >

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/weirenzheng" />
            </LinearLayout>

        </RelativeLayout>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:visibility="gone"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/text_size_12"
             android:layout_marginTop="@dimen/margin_15"
             android:layout_marginBottom="@dimen/margin_15"
             style="@style/gray_textstyle_14"
             android:autoLink="phone"
            android:text="010-85792889" />

        <LinearLayout
            android:id="@+id/linear_tel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/margin_10"
            android:background="@color/white"
            android:gravity="center"
            android:layout_marginTop="@dimen/margin_34"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/tv_sure"
                android:layout_width="@dimen/button_width_90"
                android:layout_height="@dimen/button_height_28"
                android:gravity="center"
                style="@style/gray_textstyle_14"
                android:background="@drawable/shape_black_gray"
                android:visibility="gone"
                android:text="确定" />

            <TextView
                android:id="@+id/hehe_im"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height_28"
                android:background="@drawable/shape_black_gray"
                android:drawableLeft="@drawable/erji"
                android:padding="@dimen/margin_5"
                android:gravity="center"
                android:visibility="gone"
                style="@style/gray_textstyle_14"
                android:text="在线客服" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="44dp"
                android:layout_height="wrap_content"
                android:visibility="gone"
                style="@style/gray_textstyle_14"
                android:text="" />

            <TextView
                android:id="@+id/hehe_tel"
                android:drawableLeft="@drawable/kefu"
                android:padding="@dimen/margin_5"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height_28"
                android:background="@drawable/shape_black_gray"
                android:gravity="center"
                style="@style/gray_textstyle_14"
                android:text="电话客服" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>