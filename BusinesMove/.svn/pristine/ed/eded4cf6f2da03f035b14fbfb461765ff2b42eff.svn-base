<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/pop_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/alert_titlebar_height"
        android:visibility="visible"
        android:gravity="center" >

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent" >

            <LinearLayout
                android:id="@+id/linear_back"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/iv_back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/shauxuanfanhuijian"
                    android:visibility="visible" />

            </LinearLayout>

            <TextView
                android:id="@+id/cancle_bt"
                android:layout_width="50dp"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/text_size_14"
                android:textColor="@color/color_999999"
                android:visibility="visible"
                android:text="取消" />

        </FrameLayout>

        <TextView
            android:id="@+id/production_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="@dimen/text_size_16"
                android:textColor="@color/color_333333"
            android:text="筛选" />
 
        <TextView
            android:id="@+id/sure_bt"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:textSize="@dimen/text_size_14"
            android:gravity="center"
                android:textColor="@color/color_999999"
            android:layout_marginRight="@dimen/margin_10"
            android:text="确定" />
    </LinearLayout>
<View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:layout_marginBottom="10dp"
            android:background="@color/line_gray" />
    <LinearLayout
        android:id="@+id/content_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingLeft="@dimen/margin_10" >

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/text_size_16"
                android:textColor="@color/color_666666"
            android:text="产地" />

        <LinearLayout
            android:id="@+id/country_content"
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearLayout_height_32"
            android:gravity="center" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_25"
                android:textSize="@dimen/text_size_14"
                android:textColor="@color/color_666666"
                android:text="国家" />

            <TextView
                android:id="@+id/tv_country"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin_15"
                android:layout_weight="1"
                android:gravity="right"
                android:textColor="@color/red_text_color"
                android:text="" />

            <ImageView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin_10"
                android:src="@drawable/arrow" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="@dimen/margin_10"
            android:background="@color/line_gray" />

        <LinearLayout
            android:id="@+id/area_content"
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearLayout_height_40"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_25"
                android:textSize="@dimen/text_size_14"
                android:textColor="@color/color_666666"
                android:text="产区" />

            <TextView
                android:id="@+id/tv_production"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin_15"
                android:layout_weight="1"
                android:gravity="right"
                 android:textColor="@color/red_text_color"
                android:text="" />

            <ImageView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin_10"
                android:src="@drawable/arrow" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@color/line_gray" />

        <TextView
            android:id="@+id/tv_pl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/text_size_16"
            android:textColor="@color/color_666666"
            android:visibility="gone"
            android:text="品类" />

        <LinearLayout
            android:id="@+id/linear_pl"
            android:layout_width="match_parent"
            android:gravity="center"
            android:visibility="gone"
            android:layout_height="@dimen/linearLayout_height_32" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_25"
                android:textSize="@dimen/text_size_14"
                android:textColor="@color/color_666666"
                android:text="品类" />

            <TextView
                android:id="@+id/tv_brand"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin_15"
                android:layout_weight="1"
                android:gravity="right"
                android:textColor="@color/red_text_color"
                android:text="" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin_10"
                android:src="@drawable/arrow" />
        </LinearLayout>

        <TextView
            android:id="@+id/bt_clear"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_marginTop="@dimen/margin_15"
            android:layout_gravity="center"
            android:textColor="@color/red_text_color"
            android:text="清除所有条件" />

    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ListView
            android:id="@+id/listView_country"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@null"
            android:layout_gravity="center"
            android:dividerHeight="0dp"
            android:listSelector="@null"
            android:visibility="gone" >
        </ListView>
    </FrameLayout>

</LinearLayout>