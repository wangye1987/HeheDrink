<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
         android:layout_height="120dp"
    android:orientation="horizontal">
      <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent" >

        <ImageView
            android:id="@+id/iv_image"
            android:layout_marginTop="@dimen/margin_11"
            android:layout_marginLeft="@dimen/margin_15"
            android:layout_width="98dp"
            android:layout_height="98dp" />

        <ImageView
            android:id="@+id/tv_pin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:src="@drawable/pin" />

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/margin_12"
            android:paddingTop="@dimen/margin_5"
            android:orientation="vertical" >
<LinearLayout
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/order_tip"
        android:text=""
        android:textColor="@color/white"
        android:padding="2dp"
        android:textSize="@dimen/text_size_10"
        android:background="@color/color_FF3939"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_goods"
                android:layout_marginLeft="@dimen/margin_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:maxLines="2"
                android:ellipsize="end"
                android:text="dsadad"
                android:paddingRight="@dimen/margin_15"
                android:textColor="@color/color_2b2b2b"
                android:textSize="@dimen/text_size_15" />
</LinearLayout>

            <TextView
                android:id="@+id/order_gg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/margin_5"
                android:paddingTop="@dimen/margin_5"
                android:text="规格"
                android:visibility="invisible"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/text_size_12" />

            <LinearLayout
                android:id="@+id/linear_dq"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_5"
                android:layout_marginTop="@dimen/margin_5"
                android:visibility="invisible" >

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="待付尾款:"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_size_12" />

                <TextView
                    android:id="@+id/tv_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="43"
                    android:layout_marginLeft="@dimen/margin_5"
                    android:textColor="@color/color_FF3838"
                    android:textSize="@dimen/text_size_12" />

                <TextView
                    android:id="@+id/tv_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_size_12" />
            </LinearLayout>

            <LinearLayout
                android:paddingRight="@dimen/margin_37"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/tv_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="预付订金:"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_size_12"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_dj"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dsada"
                    android:textColor="@color/color_FF3838"
                    android:textSize="@dimen/text_size_15" />

                <TextView
                    android:id="@+id/tv_unit2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dasda"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_size_15" />

                <TextView
                    android:id="@+id/tv_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="ds"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_size_12" />
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingRight="@dimen/margin_37"
                android:paddingTop="@dimen/margin_5">

                <TextView
                    android:id="@+id/tv_support"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text=""
                    android:visibility="invisible"
                    android:textColor="@color/color_af2942"
                    android:textSize="@dimen/text_size_12" />


                <TextView
                    android:id="@+id/tv_itemtotal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="center|top"
                    android:textColor="@color/color_FF3838"
                    android:textSize="@dimen/text_size_15" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@id/tv_itemtotal"
                    android:gravity="center"
                    android:text="合计:"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_size_12" />
            </RelativeLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_status_activity"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:src="@drawable/tuikuan" />

    </FrameLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/tv_error"
        android:visibility="gone"
        android:textColor="@color/color_FF3838"
        android:textSize="@dimen/text_size_10"
        android:padding="@dimen/margin_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="抱歉合伙买只支持北京购买" />

    <LinearLayout
        android:background="@color/color_fef1f3"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/linear_top_sigle_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/linearLayout_height_28"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:visibility="gone"
        android:orientation="horizontal" >
    </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_buttom_sigle"
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearLayout_height_28"
            android:gravity="center"
            android:visibility="gone"
            android:orientation="horizontal" >
    </LinearLayout>
    
</LinearLayout>
  <View android:layout_width="match_parent"
        android:layout_height="5dp"
      android:background="@color/color_f1f1f1"
      />

</LinearLayout>
