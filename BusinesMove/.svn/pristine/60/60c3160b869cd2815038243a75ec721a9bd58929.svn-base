<?xml version="1.0" encoding="utf-8"?>
<!-- 商品详情页面布局 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tv_find"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white" >

    <!-- <com.heheys.ec.view.ScrollViewPullLargen -->

    <com.heheys.ec.view.UserDefineScrollView
        android:id="@+id/product_detail_top_parent"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical" >

            <!-- 图片浏览页面 -->

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <com.heheys.ec.lib.view.CustomViewPager
                    android:id="@+id/product_detail_vp"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/image_height_280"
                    android:visibility="gone"
                    android:layout_gravity="center" >
                </com.heheys.ec.lib.view.CustomViewPager>

                <LinearLayout
                    android:id="@+id/time"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="@dimen/margin_28"
                    android:background="@drawable/xianshibeijing"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="invisible" >

                    <ImageView
                        android:id="@+id/imageView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingLeft="2dp"
                        android:src="@drawable/zhomgbiao" />

                    <TextView
                        android:id="@+id/product_day"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
