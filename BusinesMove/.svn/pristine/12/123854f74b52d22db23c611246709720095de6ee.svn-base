<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
        android:background="@drawable/shape_white_round"
    android:layout_height="match_parent"
    android:orientation="vertical" >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearLayout_height_48"
            android:gravity="center" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/iv_delete_provice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/delete_province"
                        android:visibility="invisible" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_privoice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                	android:paddingLeft="22dp"
                	android:paddingRight="10dp"
                    android:textColor="@color/color_565656"
                    android:textSize="@dimen/text_size_13" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/tv_edit"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="编辑"
                android:textSize="@dimen/text_size_13"
                android:layout_marginRight="@dimen/margin_8"
                android:textColor="@color/color_f9883d" />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line_gray" />

        <com.heheys.ec.lib.view.GridViewScrollview
            android:id="@+id/gridview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp"
            android:listSelector="@android:color/transparent"
            android:numColumns="4" >
        </com.heheys.ec.lib.view.GridViewScrollview>

</LinearLayout>