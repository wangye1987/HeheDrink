package com.heheys.ec.view;

import com.heheys.ec.controller.listener.IScrollListener;

import android.widget.GridView;

/**
 * 
 * Describe:GridView滑动控制
 * 
 * Date:2015-9-29
 * 
 * Author:liuzhouliang
 */

public class GridViewController extends BaseViewScrollController {

	private GridView mGridView = null;

	public GridViewController(GridView gridView, IScrollListener iScrollListener) {
		mGridView = gridView;
		mGridView.setOnScrollListener(this);
		this.iScrollListener = iScrollListener;
	}

}
