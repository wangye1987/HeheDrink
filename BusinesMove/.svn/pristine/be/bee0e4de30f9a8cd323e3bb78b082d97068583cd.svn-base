<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
   >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center" >

        <LinearLayout
            android:id="@+id/linear_check"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center" >

            <CheckBox
                android:id="@+id/iv_check"
                style="@style/CheckBoxStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/order_margin_11"
                android:checked="true"
                android:gravity="center"
                android:visibility="invisible" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_center"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_14"
            android:layout_marginRight="@dimen/order_margin_11"
            android:layout_weight="1"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/linear_shop"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_10"
                android:gravity="center|left"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/tv_shop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/yellow_text_down"
                    android:textSize="@dimen/text_size_16" />

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_40"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_tel1"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电话一  : "
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />

                <TextView
                    android:id="@+id/tv_tel1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_tel2"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:visibility="gone" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电话二 : "
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />

                <TextView
                    android:id="@+id/tv_tel2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_tel3"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center|left"
                android:visibility="gone" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电话三 : "
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />

                <TextView
                    android:id="@+id/tv_tel3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_tel4"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center|left"
                android:visibility="gone" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电话四 : "
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />

                <TextView
                    android:id="@+id/tv_tel4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_tel5"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center|left"
                android:visibility="gone" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电话五 : "
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />

                <TextView
                    android:id="@+id/tv_tel5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_address"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="地址 : "
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />

                <TextView
                    android:id="@+id/tv_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_remark"
                style="@style/marginagent_topbuttom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_10"
                android:gravity="center|left" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="备注 : "
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />

                <TextView
                    android:id="@+id/tv_remark"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="@color/color_979797"
                    android:textSize="@dimen/text_size_13" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_tel"
            android:layout_width="@dimen/margin_84"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal" >

            <View
                android:id="@+id/tv_line"
                android:layout_width="0.5dip"
                android:layout_height="fill_parent"
                android:layout_gravity="left|center_vertical"
                android:layout_marginBottom="@dimen/order_margin_17"
                android:layout_marginTop="@dimen/order_margin_17"
                android:background="@color/color_D3D3D3" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center" >

                <ImageView
                    android:id="@+id/iv_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/selector_tel" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <com.heheys.ec.lib.view.DrawLineTextView
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:layout_marginBottom="2dip"
        android:background="@color/line_gray" />

</LinearLayout>