public class ProductDetailInfor implements Serializable {

	private static final long serialVersionUID = 1L;
	private String status;// 状态码
	private ProductInfor result;// 返回结果
	private ErrorBean error;// 返回失败结果

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public ProductInfor getResult() {
		return result;
	}

	public void setResult(ProductInfor result) {
		this.result = result;
	}

	public ErrorBean getError() {
		return error;
	}

	public void setError(ErrorBean error) {
		this.error = error;
	}

	@Override
	public String toString() {
		return "ProductDetailInfor [status=" + status + ", result=" + result
				+ ", error=" + error + "]";
	}

